%h1 New user

= error_messages_for :user 

- form_for(@user) do |f| 
  %p
    %b Username
    %br
    = f.text_field :username 
  %p
    %b Email
    %br
    = f.text_field :email 
  %p
    %b Password
    %br
    = f.password_field :password 
  %p
    %b Password Confirm
    %br
    = f.password_field :password_confirmation
  %p
    %b OpenID
    %br
    = f.text_field :openid_identifier 
  %p
    = f.submit "Create"
    
= link_to 'Back', users_path 